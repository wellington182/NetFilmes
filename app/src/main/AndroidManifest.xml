<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dominando.android.netfilmes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FilmesActivity" />
        <activity android:name=".GenerosActivity" />
        <activity android:name=".CadastrarFilmeActivity" />
        <activity android:name=".CadastrarGeneroActivity" />
        <activity android:name=".ListarFilmesActivity" />
        <activity android:name=".ItemFilmeActivity" />
        <activity android:name=".AtualizarFilmeActivity" />
        <activity android:name=".ListarFilmesGeneroActivity" />
        <activity android:name=".ListarGenerosActivity" />
        <activity android:name=".ItemGeneroActivity" />
        <activity android:name=".AtualizarGeneroActivity"></activity>
    </application>

</manifest>